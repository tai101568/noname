<ul class="mws-summary clearfix">

<% for(var i=0; i<outputs.length; i++) {%>
   <li class="mws-stat no-padding">
   	<!-- Statistic Icon (edit to change icon) -->
   	<span class="mws-stat-icon <%= outputs[i].dataType.cssIcon %>"></span>
   	
   	<!-- Statistic Content -->
    <span class="mws-stat-content" name="<%= outputs[i].referenceName %>_container">
    	<span class="mws-stat-title blurred-item"><%= outputs[i].dataType.name %></span>
        <span class="mws-stat-value blurred-item">
        	<span name="<%= outputs[i].referenceName %>">----</span>
       	</span>
       	<i name="<%= outputs[i].referenceName %>" class="icol-chart-curve" title="Click to see historical data" style="cursor:pointer;z-index:1000"></i>
    </span>
<% } %>