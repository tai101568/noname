<ul class="mws-summary clearfix">

<% for(var i=0; i<rows.length; i++) {%>
   <li class="mws-stat no-padding">
   	<!-- Statistic Icon (edit to change icon) -->
   	<span class="mws-stat-icon <%= rows[i].cssIcon %>"></span>
   	
   	<!-- Statistic Content -->
    <span class="mws-stat-content" name="<%= rows[i].referenceName %>_container">
    	<span class="mws-stat-title blurred-item"><%= rows[i].title %></span>
        <span class="mws-stat-value blurred-item">
        	<span name="<%= rows[i].referenceName %>">----</span>
       	</span>
       	<i name="<%= rows[i].referenceName %>" name="<%= rows[i].referenceName %>" class="icol-chart-curve" title="Click to see historical data" style="cursor:pointer"></i>
    </span>
<% } %>